url = {http://www.opticsinfobase.org/abstract.cfm?URI=josa-55-10-1205},
doi = {10.1364/JOSA.55.001205},
abstract = {The index of refraction of optical quality fused silica (SiO2) was determined for 60 wavelengths from 0.21 to 3.71 {\textmu} at 20{\textdegree}C. The dispersion equation \[equation\] where $\lambda$ is expressed in microns was found to yield an absolute residual of 10.5{\texttimes}10-6. The variation in index between 12 specimens was determined. Dispersive properties of the material and thermal coefficient of index are graphically presented. A comparison with previous NBS index data is discussed.},
}


